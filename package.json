    "ngx-cookie-service": "^1.0.8",
    "reselect": "^2.5.4",
    "rxjs": "^5.3.0",
    "typescript": "2.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^1.1.2",
    "@angular/compiler-cli": "^4.2.6",
    "@angularclass/hmr": "^1.2.2",
    "@compodoc/compodoc": "^1.0.0-beta.8",
    "@ngrx/store-devtools": "^3.2.3",
    "@types/grecaptcha": "^2.0.31",
    "@types/jasmine": "2.5.38",
    "@types/morris.js": "^0.5.6",
    "@types/node": "~6.0.60",
